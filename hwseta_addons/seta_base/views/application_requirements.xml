<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="seta_application_requirements_tree_view" model="ir.ui.view">
		<field name="name">seta.application.requirements.tree.view</field>
		<field name="model">seta.application.requirements</field>
		<field name="arch" type="xml">
			<tree>
				<field name="name"/>
			</tree>
		</field>
	</record>
	<record id="seta_application_requirements_form_view" model="ir.ui.view">
		<field name="name">seta.application.requirements.form.base</field>
		<field name="model">seta.application.requirements</field>
		<field name="arch" type="xml">
			<form>
				<header>
                </header>
				<sheet>
					<group name="group_parent">
						<group name="group_left">
							<field name="name"/>
						</group>
						<group name="group_right">
							<field name="code"/>
						</group>
						<field name="display" widget="html"/>
					</group>
				</sheet>
<!--				<div class="oe_chatter">-->
<!--					<field name="message_follower_ids" widget="mail_followers" />-->
<!--					<field name="activity_ids" widget="mail_activity" />-->
<!--					<field name="message_ids" widget="mail_thread" />-->
<!--				</div>-->
			</form>
		</field>
	</record>
	<record id="seta_application_requirements_action" model="ir.actions.act_window">
		<field name="name">seta.application.requirements</field>
		<field name="res_model">seta.application.requirements</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">tree,form</field>
	</record>
	<menuitem
			id="menu_seta_application_requirements"
			sequence="1"
			name="application requirements"
			parent="seta_base.config_root"
			action="seta_application_requirements_action"/>
</odoo>