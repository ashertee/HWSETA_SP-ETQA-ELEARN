	<record id="citizen_resident_status_code_list_view" model="ir.ui.view">
		<field name="name">citizen.resident.status.code.list.view</field>
		<field name="model">citizen.resident.status.code</field>
		<field name="arch" type="xml">
			<list  duplicate="false">
		
                <field name="name"/>
                <field name="setmis_lookup" string="SETMIS Lookup"/>
                <field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
			
		</field>
	</record>

	<record id="citizen_resident_status_code_form_view" model="ir.ui.view">
		<field name="name">citizen.resident.status.code.form.view</field>
		<field name="model">citizen.resident.status.code</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
			
			    
                <sheet>
                    <group>
                       <group string="Lookup Details">
                               <field name="name"/>
                               <field name="setmis_lookup" string="SETMIS Lookup"/>
                               <field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="citizen_resident_status_code_action" model="ir.actions.act_window">
		<field name="name">Citizen Resident Status Code</field>
		<field name="res_model">citizen.resident.status.code</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_citizen_resident_status_code" parent="master_menu_seta_lookup" sequence= "1" name= "Citizen Resident Status Code" action="citizen_resident_status_code_action"/>

</odoo>