	<record id="pivotal_programme_qualification_list_view" model="ir.ui.view">
		<field name="name">pivotal.programme.qualification.list.view</field>
		<field name="model">pivotal.programme.qualification</field>
		<field name="arch" type="xml">
			<list  duplicate="false" delete ="false">
			
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="pivotal_programme_qualification_form_view" model="ir.ui.view">
		<field name="name">pivotal.programme.qualification.form.view</field>
		<field name="model">pivotal.programme.qualification</field>
		<field name="arch" type="xml">
			<form duplicate="false" >
			
			    
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="pivotal_programme_qualification_action" model="ir.actions.act_window">
		<field name="name">Pivotal Programme Qualification</field>
		<field name="res_model">pivotal.programme.qualification</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_pivotal_programme_qualification" parent="master_menu_seta_lookup" sequence= "1" name= "Pivotal Programme Qualification" action="pivotal_programme_qualification_action"/>

</odoo>