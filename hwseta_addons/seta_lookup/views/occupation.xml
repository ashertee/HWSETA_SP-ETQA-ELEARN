<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="ofo_code_occupation_lookup_list_view" model="ir.ui.view">
		<field name="name">Occupation list</field>
		<field name="model">ofo.code.occupation.lookup</field>
		<field name="arch" type="xml">
			<list create="false">
<field name="name"/>

			</list>
		</field>
	</record>

	<record id="ofo_code_occupation_lookup_form_view" model="ir.ui.view">
		<field name="name">Occupation Form</field>
		<field name="model">ofo.code.occupation.lookup</field>
		<field name="arch" type="xml">
			<form create="false" edit="false">
                <sheet>
                    <group>
                       <group string="Lookup Details">
                               <field name="name"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="ofo_code_occupation_lookup_action" model="ir.actions.act_window">
		<field name="name">Occupation</field>
		<field name="res_model">ofo.code.occupation.lookup</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_aet_level" parent="master_menu_seta_lookup" sequence= "99" name= "ofo occupation" action="ofo_code_occupation_lookup_action"/>

</odoo>
