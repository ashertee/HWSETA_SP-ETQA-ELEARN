	<record id="nationality_code_list_view" model="ir.ui.view">
		<field name="name">nationality.code.list.view</field>
		<field name="model">nationality.code</field>
		<field name="arch" type="xml">
			<list  delete="false" duplicate="false">
				<field name="name"/>
   				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="nationality_code_form_view" model="ir.ui.view">
		<field name="name">nationality.code.form.view</field>
		<field name="model">nationality.code</field>
		<field name="arch" type="xml">
			<form  delete="false" duplicate="false">
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="nationality_code_action" model="ir.actions.act_window">
		<field name="name">Nationality Code</field>
		<field name="res_model">nationality.code</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_nationality_code" parent="master_menu_seta_lookup" sequence= "1" name= "Nationality Code" action="nationality_code_action"/>

</odoo>