	<record id="res_city_list_view" model="ir.ui.view">
		<field name="name">res.city.list.view</field>
		<field name="model">res.city</field>
		<field name="arch" type="xml">
			<list  duplicate="false">
	
				<field name="name"/>
			

			</list>
		</field>
	</record>

	<record id="res_city_form_view" model="ir.ui.view">
		<field name="name">res.city.form.view</field>
		<field name="model">res.city</field>
		<field name="arch" type="xml">
			<form >
				<!-- <header>
                    <button name="create_record" string="Create"   type="object" class="btn-primary" />
                    <button name="edit_record" string="Edit"  type="object" class="btn-primary" />
                            
                </header> -->
			    
                <sheet>
                    <group>
						<group string="Lookup Details">
                               <field name="name"/>
                    
							<!--<field name="district_id"/>-->
							<field name="province_id" options="{'no_open': True}"/>
							<field name="country_id" options="{'no_open': True}"/>
							<!--<field name="urban_rural"/>-->
						
							<field name="latitude"/>
							<field name="longitude"/>
						</group>
                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="res_city_action" model="ir.actions.act_window">
		<field name="name">City</field>
		<field name="res_model">res.city</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_res_city" parent="master_menu_seta_lookup" sequence= "1" name= "City" action="res_city_action"/>

</odoo>