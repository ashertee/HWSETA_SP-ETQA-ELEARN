	<record id="home_language_code_list_view" model="ir.ui.view">
		<field name="name">home.language.code.list.view</field>
		<field name="model">home.language.code</field>
		<field name="arch" type="xml">
			<list duplicate="false">
			
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="home_language_code_form_view" model="ir.ui.view">
		<field name="name">home.language.code.form.view</field>
		<field name="model">home.language.code</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
				            <field name="nlrd_lookup" string="NLRD Lookup"/>
                              
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="home_language_code_action" model="ir.actions.act_window">
		<field name="name">Home Language Code</field>
		<field name="res_model">home.language.code</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_home_language_code" parent="master_menu_seta_lookup" sequence= "1" name= "Home Language Code" action="home_language_code_action"/>

</odoo>