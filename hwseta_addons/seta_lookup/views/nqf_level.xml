	<record id="nqf_level_list_view" model="ir.ui.view">
		<field name="name">nqf.level.list.view</field>
		<field name="model">nqf.level</field>
		<field name="arch" type="xml">
			<list  duplicate="false" delete="false">
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="nqf_level_form_view" model="ir.ui.view">
		<field name="name">nqf.level.form.view</field>
		<field name="model">nqf.level</field>
		<field name="arch" type="xml">
			<form  delete="false" duplicate="false">

                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="nqf_level_action" model="ir.actions.act_window">
		<field name="name">NQF Level ID</field>
		<field name="res_model">nqf.level</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_nqf_level" parent="master_menu_seta_lookup" sequence= "1" name= "NQF Level ID" action="nqf_level_action" />

</odoo>