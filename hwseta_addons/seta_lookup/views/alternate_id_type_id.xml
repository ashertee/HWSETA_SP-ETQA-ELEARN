	<record id="alternate_id_type_id_list_view" model="ir.ui.view">
		<field name="name">alternate.id.type.id.list.view</field>
		<field name="model">alternate.id.type.id</field>
		<field name="arch" type="xml">
			<list edit="false" duplicate="false" >
				<field name="name"/>
				<field name="setmis_lookup" string="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="alternate_id_type_id_form_view" model="ir.ui.view">
		<field name="name">alternate.id.type.id.form.view</field>
		<field name="model">alternate.id.type.id</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
		
			    
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
							<field name="setmis_lookup" string="SETMIS Lookup"/>
				            <field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="alternate_id_type_id_action" model="ir.actions.act_window">
		<field name="name">Alternate ID Type ID</field>
		<field name="res_model">alternate.id.type.id</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_alternate_id_type_id" parent="master_menu_seta_lookup" sequence= "1" name= "Alternate ID Type ID" action="alternate_id_type_id_action"/>

</odoo>