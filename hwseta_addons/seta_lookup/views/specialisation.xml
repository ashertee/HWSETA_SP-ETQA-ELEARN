<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="ofo_code_specialisation_list_view" model="ir.ui.view">
		<field name="name">ofo.code.specialisation.list.view</field>
		<field name="model">ofo.code.specialisation.lookup</field>
		<field name="arch" type="xml">
			<list create="false">
<field name="name"/>

			</list>
		</field>
	</record>

	<record id="ofo_code_specialisation_form_view" model="ir.ui.view">
		<field name="name">ofo.code.specialisation.form.view</field>
		<field name="model">ofo.code.specialisation.lookup</field>
		<field name="arch" type="xml">
			<form create="false" edit="false">
                <sheet>
                    <group>
                       <group string="Lookup Details">
                               <field name="name"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="specialisation_action" model="ir.actions.act_window">
		<field name="name">Specialisation</field>
		<field name="res_model">ofo.code.specialisation.lookup</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_aet_level" parent="master_menu_seta_lookup" sequence= "1" name= "specialisation" action="specialisation_action"/>

</odoo>
