	<record id="etqe_id_list_view" model="ir.ui.view">
		<field name="name">etqe.id.list.view</field>
		<field name="model">etqe.id</field>
		<field name="arch" type="xml">
		
			<list  duplicate="false">

				<field name="name"/>
				<field name ="etqe_id" string="ETQE_ID"/>
				<field name="mnemonic" string="Mnemonic"/>
				<field name="setmis_lookup" string="SETMIS Lookup"/>
                <field name="nlrd_lookup" string ="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="etqe_id_form_view" model="ir.ui.view">
		<field name="name">etqe.id.form.view</field>
		<field name="model">etqe.id</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
			
			    
                <sheet>
                    <group>
                       <group string="Lookup Details">
                              <field name="name"/>
							<field name ="etqe_id" string="ETQE_ID"/>
							<field name="mnemonic" string="Mnemonic"/>
							<field name="setmis_lookup" string="SETMIS Lookup"/>
                            <field name="nlrd_lookup" string ="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="etqe_id_action" model="ir.actions.act_window">
		<field name="name">ETQE ID</field>
		<field name="res_model">etqe.id</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_etqe_id" parent="master_menu_seta_lookup" sequence= "1" name= "ETQE ID" action="etqe_id_action"/>

</odoo>