	<record id="OFO_Code_list_view" model="ir.ui.view">
		<field name="name">ofo.code.list.view</field>
		<field name="model">ofo.code</field>
		<field name="arch" type="xml">
			<list  delete="false" duplicate="false">
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="OFO_Code_form_view" model="ir.ui.view">
		<field name="name">ofo.code.form.view</field>
		<field name="model">ofo.code</field>
		<field name="arch" type="xml">
			<form  delete="false" duplicate="false">

                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                           
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="OFO_Code_action" model="ir.actions.act_window">
		<field name="name">OFO Code</field>
		<field name="res_model">ofo.code</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_OFO_Code" parent="master_menu_seta_lookup" sequence= "1" name= "OFO Code" action="OFO_Code_action"/>


</odoo>