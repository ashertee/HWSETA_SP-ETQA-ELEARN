	<record id="scheme_year_code_list_view" model="ir.ui.view">
		<field name="name">scheme.year.code.list.view</field>
		<field name="model">scheme.year.code</field>
		<field name="arch" type="xml">
			<list  delete="0"  duplicate="0">
			
                <field name="name" readonly="1"/>
                
               

			</list>
		</field>
	</record>

	<record id="scheme_year_code_form_view" model="ir.ui.view">
		<field name="name">scheme.year.code.form.view</field>
		<field name="model">scheme.year.code</field>
		<field name="arch" type="xml">
			<form delete="0"   duplicate="0">
		
                <sheet>
                    <group>
						<group string="Lookup Details">
                               <field name="name" readonly="1" options="{'no_create': True, 'no_create_edit':True,'no_open':True}"/>
                              
						</group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="scheme_year_code_action" model="ir.actions.act_window">
		<field name="name">Scheme Year Code</field>
		<field name="res_model">scheme.year.code</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>                                                                                     
	<menuitem id="menu_item_Scheme_year" parent="master_menu_seta_lookup" sequence= "1" name= "Scheme Year" action="scheme_year_code_action" />

</odoo>