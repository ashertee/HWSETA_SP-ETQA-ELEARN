	<record id="last_school_year_list_view" model="ir.ui.view">
		<field name="name">last.school.year.list.view</field>
		<field name="model">last.school.year</field>
		<field name="arch" type="xml">
			<list  duplicate="false">

				<field name="name"/>

			</list>
		</field>
	</record>

	<record id="last_school_year_form_view" model="ir.ui.view">
		<field name="name">last.school.year.form.view</field>
		<field name="model">last.school.year</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
			    
                <sheet>
                    <group>
                       <group string="Lookup Details">
                               <field name="name"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="last_school_year_action" model="ir.actions.act_window">
		<field name="name">Last School Year</field>
		<field name="res_model">last.school.year</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_last_school_year" parent="master_menu_seta_lookup" sequence= "1" name= "Last School Year" action="last_school_year_action"/>

</odoo> 