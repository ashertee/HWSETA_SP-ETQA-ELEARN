	<record id="highest_education_list_view" model="ir.ui.view">
		<field name="name">highest.education.list.view</field>
		<field name="model">highest.education</field>
		<field name="arch" type="xml">
			<list  duplicate="false">
			
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="highest_education_form_view" model="ir.ui.view">
		<field name="name">highest.education.form.view</field>
		<field name="model">highest.education</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="highest_education_action" model="ir.actions.act_window">
		<field name="name">Highest Education</field>
		<field name="res_model">highest.education</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_highest_education" parent="master_menu_seta_lookup" sequence= "1" name= "Highest Education" action="highest_education_action"/>

</odoo>