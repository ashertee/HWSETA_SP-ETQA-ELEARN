<?xml version="1.0" encoding="utf-8"?>
<odoo>

	<record id="ofo_code_lookup_list_view" model="ir.ui.view">
		<field name="name">OFO list</field>
		<field name="model">ofo.code.lookup</field>
		<field name="arch" type="xml">
			<list>
                               <field name="name"/>
                               <field name="ofo_code"/>
                               <field name="ofo_year"/>
                               <field name="occupation"/>
                               <field name="specialisation"/>
			</list>
		</field>
	</record>

	<record id="ofo_code_lookup_form_view" model="ir.ui.view">
		<field name="name">OFO Form</field>
		<field name="model">ofo.code.lookup</field>
		<field name="arch" type="xml">
			<!-- <form create="false" edit="false"> -->
			<form>
        <header>
        </header>
                <sheet>
                    <group>
                       <group string="Lookup Details">
                               <field name="name"/>
                               <field name="ofo_code"/>
                               <field name="ofo_year"/>
                               <field name="occupation"/>
                               <field name="specialisation"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="ofo_code_lookup_action" model="ir.actions.act_window">
		<field name="name">OFO</field>
		<field name="res_model">ofo.code.lookup</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_aet_level" parent="master_menu_seta_lookup" sequence= "1" name= "ofo code" action="ofo_code_lookup_action"/>

</odoo>
