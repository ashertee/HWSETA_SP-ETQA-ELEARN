	<record id="subfield_id_list_view" model="ir.ui.view">
		<field name="name">subfield.id.list.view</field>
		<field name="model">subfield.id</field>
		<field name="arch" type="xml">
			<list  duplicate="false" delete="false">
			
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

	<record id="subfield_id_form_view" model="ir.ui.view">
		<field name="name">subfield.id.form.view</field>
		<field name="model">subfield.id</field>
		<field name="arch" type="xml">
			<form  duplicate="false" delete="false">
	
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="subfield_id_action" model="ir.actions.act_window">
		<field name="name">Subfield ID</field>
		<field name="res_model">subfield.id</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_subfield_id" parent="master_menu_seta_lookup" sequence= "1" name= "Subfield ID" action="subfield_id_action"/>

</odoo>