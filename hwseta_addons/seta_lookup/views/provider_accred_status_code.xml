	<record id="provider_accred_status_code_list_view" model="ir.ui.view">
		<field name="name">provider.accred.status.code.list.view</field>
		<field name="model">provider.accred.status.code</field>
		<field name="arch" type="xml">
			<list  duplicate="false">
				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>

			</list>
		</field>
	</record>

