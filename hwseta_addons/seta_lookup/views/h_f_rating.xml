	<record id="h_f_rating_list_view" model="ir.ui.view">
		<field name="name">h.f.rating.list.view</field>
		<field name="model">h.f.rating</field>
		<field name="arch" type="xml">
			<list  duplicate="false">

				<field name="name"/>
				<field name="setmis_lookup" string ="SETMIS Lookup"/>
				<field name="nlrd_lookup" string="NLRD Lookup"/>
				
			</list>
		</field>
	</record>

	<record id="h_f_rating_form_view" model="ir.ui.view">
		<field name="name">h.f.rating.form.view</field>
		<field name="model">h.f.rating</field>
		<field name="arch" type="xml">
			<form  duplicate="false">
		
                <sheet>
                    <group>
                       <group string="Lookup Details">
                            <field name="name"/>
                            <field name="setmis_lookup" string ="SETMIS Lookup"/>
							<field name="nlrd_lookup" string="NLRD Lookup"/>
                      </group>

                    </group>
                </sheet>
			</form>
		</field>
	</record>

	<record id="h_f_rating_action" model="ir.actions.act_window">
		<field name="name">H F Rating</field>
		<field name="res_model">h.f.rating</field>
		<field name="type">ir.actions.act_window</field>
		<field name="context">{}</field>
		<field name="view_mode">list,form</field>
	</record>
	<menuitem id="master_sub_menu_h_f_rating" parent="master_menu_seta_lookup" sequence= "1" name= "H F Rating" action="h_f_rating_action"/>

</odoo>