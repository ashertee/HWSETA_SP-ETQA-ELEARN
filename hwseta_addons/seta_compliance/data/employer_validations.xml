            <field name="validation_type" ref="seta_compliance.regex_findall_not_in" />
            <field name="validation_value">(0123|1234|2345|3456|4567|5678|6789|3210|4321|5432|6543|7654|8765|9876|0000|1111|2222|3333|4444|5555|6666|7777|8888|9999)</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should not contain 4 consecutive numbers.</field>
