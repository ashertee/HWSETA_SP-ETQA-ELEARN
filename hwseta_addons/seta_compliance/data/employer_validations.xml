        <record id="val_business_validation_empl" model="seta.map.line.validation">
            <field name="name">Forbiden Characters</field>
            <field name="validation_type" ref="seta_compliance.regex_match" />
            <field name="validation_value">\bUNKNOWN\b|\bAS\b|\bABOVE\b|\bSOOS\b|\bBO\b|\bDELETE\b|\bN[/]A\b|\bNA\b|\bU\b|\bNONE\b|\bGEEN\b|\b0\b|\bTEST\b|\bONTBREEK\b|\bNIL\b|^[-]$|[–]</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should not contain %UNKNOWN% or %AS ABOVE% or %SOOS
                                BO% or %DELETE% or N/A or NA or U or NONE or GEEN or 0 or TEST or %ONTBREEK% or NIL or - or –
            </field>
        </record>

