        <record id="val_latitude_degree" model="seta.map.line.validation">
            <field name="name">Latitude Degree_NEW</field>
            <field name="validation_type" ref="seta_compliance.regex_full_match" />
            <field name="validation_value">^-(3[0-5]|2[2-9])$</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should only contain whole numbers and may not be greater than -22 and may not have a value less than -35</field>
        </record>

