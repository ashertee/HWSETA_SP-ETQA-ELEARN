            <field name="name">Latitude Minutes</field>
            <field name="validation_type" ref="seta_compliance.regex_full_match" />
            <field name="validation_value">^(?:[0-9]|[1-5][0-9])$</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should only contain whole numbers and should not be greater than 59</field>
        </record>

