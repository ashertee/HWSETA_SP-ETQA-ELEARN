        <record id="val_employer_sdl_no" model="seta.map.line.validation">
            <field name="name">Employer SDL Number</field>
            <field name="validation_type" ref="seta_compliance.regex_match" />
            <field name="validation_value">\b[L][0-9]{9}\b|\b[N][0-9]{9}\b|\b[T][0-9]{9}\b|\b[G][0-9]{9}\b</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should only contain L0123456789 or N0123456789.</field>
        </record>

