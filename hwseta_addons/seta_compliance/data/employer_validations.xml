            <field name="validation_type" ref="seta_compliance.regex_full_match" />
            <field name="validation_value">^(0[0-9]|1[0-9]|2[0-9]|3[0-9]|4[0-9]|5[0-9])\.([0-9]){3}$</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should only contain 1234567890 and must have a length of exactly 6 (nn.nnn) and may not be greater than 59.999.</field>
        </record>

