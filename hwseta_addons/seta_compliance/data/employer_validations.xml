            <field name="validation_value">[^A-Za-z-1234567890@#&amp;+() /\\:._,'`-]</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should only contain ABCDEFGHIJKLMNOPQRTSUVWXYZ1234567890@#&amp;+() /\:._-</field>
        </record>

