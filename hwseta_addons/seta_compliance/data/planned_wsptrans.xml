<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">

        <!-- Planned -->

        <record id="planned_map" model="seta.map">
            <field name="name">Planned</field>
            <field name="odoo_model">planned.wsptrans</field>
            <field name="setmis_version">1.01</field>
            <field name="nlrd_version">2.0</field>
        </record>

        <!--        validations-->
<!--        planned_Planned_Last_Name-->
        <record id="val_planned_Planned_Last_Name" model="seta.map.line.validation">
            <field name="name">Last Name Regex</field>
            <field name="validation_type" ref="seta_compliance.regex_findall_not_in" />
            <field name="validation_value">[^A-Za-z-1234567890#&amp;() '`]</field>
            <field name="business_validation_value">\bUNKNOWN\b|\bAS\b|\bABOVE\b|\bSOOS\b|\bBO\b|\bDELETE\b|\bN[/]A\b|\bNA\b|\bU\b|\bNONE\b|\bGEEN\b|\b0\b|\bTEST\b|\bONTBREEK\b|\bNIL\b|^[-]$|[–]</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should contain only ABCDEFGHIJKLMNOPQRTSUVWXYZ`' -</field>
            <field name="business_error_message">may not contain characters %UNKNOWN% or %AS ABOVE% or %SOOS
BO% or %DELETE% or N/A or NA or U or NONE or GEEN or 0 or TEST or %ONTBREEK% or NIL or - or
–</field>
        </record>
        <!--        planned_Planned_First_Name-->
        <record id="val_planned_Planned_First_Name" model="seta.map.line.validation">
            <field name="name">First Name Regex</field>
            <field name="validation_type" ref="seta_compliance.regex_findall_not_in" />
            <field name="validation_value">[^A-Za-z-1234567890#&amp;() '`]</field>
            <field name="business_validation_value">\bUNKNOWN\b|\bAS\b|\bABOVE\b|\bSOOS\b|\bBO\b|\bDELETE\b|\bN[/]A\b|\bNA\b|\bU\b|\bNONE\b|\bGEEN\b|\b0\b|\bTEST\b|\bONTBREEK\b|\bNIL\b|^[-]$|[–]</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should contain only ABCDEFGHIJKLMNOPQRTSUVWXYZ`' -</field>
            <field name="business_error_message">may not contain characters %UNKNOWN% or %AS ABOVE% or %SOOS
BO% or %DELETE% or N/A or NA or U or NONE or GEEN or 0 or TEST or %ONTBREEK% or NIL or - or
–</field>
        </record>
        <!--        planned_national_id-->
        <record id="val_planned_employee_id" model="seta.map.line.validation">
            <field name="name">ID Number Regex</field>
            <field name="validation_type" ref="seta_compliance.regex_match" />
            <field name="validation_value">(^[0-9]{2})(0[1-9]|1[0-2])(0[1-9]|1[0-9]|2[0-9]|3[0-1])([0-9]{4})([0-1]{1})([0-9]{2})</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should contain only 1234567890</field>
        </record>

        <!--        planned_national_id-->
        <record id="val_planned_planned_email_address" model="seta.map.line.validation">
            <field name="name">email Regex</field>
            <field name="validation_type" ref="seta_compliance.regex_full_match" />
            <field name="validation_value">\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Z|a-z]{2,}\b</field>
            <field name="system_applied">setmis</field>
            <field name="error_message">should contain a valid email</field>
        </record>

        <!--        planned_planned_cost_per_learner-->
        <!-- <record id="val_planned_planned_cost_per_learner" model="seta.map.line.validation"> -->
        <!--     <field name="name">cost per learner Regex</field> -->
        <!--     <field name="validation_type" ref="seta_compliance.regex_full_match" /> -->
        <!--     <field name="validation_value">\b[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Z|a-z]{2,}\b</field> -->
        <!--     <field name="system_applied">setmis</field> -->
        <!--     <field name="error_message">should contain a valid email</field> -->
        <!-- </record> -->
<record id="val_planned_planned_cost_per_learner" model="seta.map.line.validation">
<field name="name">Cost per learner numeric validation</field>
<field name="validation_type" ref="seta_compliance.regex_full_match" />
<field name="validation_value">^\d+$</field>
<field name="system_applied">setmis</field>
<field name="error_message">Cost per learner must only contain numbers.</field>
</record>




        <record id="planned_employee_id" model="seta.map.line">
            <field name="map_id" ref="seta_compliance.planned_map"/>
            <field name="validation_ids" eval="[(4,ref('seta_compliance.val_planned_employee_id'))]"/>
            <field name="odoo_field_type">Char</field>
            <field name="odoo_label">Employee ID</field>
            <field name="odoo_field">employee_id</field>
            <field name="model_id">planned.wsptrans</field>
        </record>

        <record id="planned_first_name" model="seta.map.line">
            <field name="map_id" ref="seta_compliance.planned_map"/>
            <field name="validation_ids" eval="[(4,ref('seta_compliance.val_planned_Planned_First_Name'))]"/>
            <field name="odoo_field_type">Char</field>
            <field name="odoo_label">First Name</field>
            <field name="odoo_field">name</field>
            <field name="model_id">planned.wsptrans</field>
        </record>

        <record id="planned_last_name" model="seta.map.line">
            <field name="map_id" ref="seta_compliance.planned_map"/>
            <field name="validation_ids" eval="[(4,ref('seta_compliance.val_planned_Planned_Last_Name'))]"/>
            <field name="odoo_field_type">Char</field>
            <field name="odoo_label">Last Name</field>
            <field name="odoo_field">surname</field>
            <field name="model_id">planned.wsptrans</field>
        </record>

        <record id="planned_cost_per_learner" model="seta.map.line">
            <field name="map_id" ref="seta_compliance.planned_map"/>
            <field name="validation_ids" eval="[(4,ref('seta_compliance.val_planned_planned_cost_per_learner'))]"/>
            <field name="odoo_field_type">Char</field>
            <field name="odoo_label">Cost Per Learner</field>
            <field name="odoo_field">cost_per_learner</field>
            <field name="model_id">planned.wsptrans</field>
        </record>



            </data>
        </odoo>
