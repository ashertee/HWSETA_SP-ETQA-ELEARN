<odoo>
    <data noupdate="1">
        <record id="person_transaction_seq" model="ir.sequence">
            <field name="name">seta.person.transaction sequence</field>
            <field name="code">seta.person.transaction</field>
            <field name="prefix">SPT</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="person_disable_transaction_seq" model="ir.sequence">
            <field name="name">seta.person.disable.transaction sequence</field>
            <field name="code">person.disable.transaction</field>
            <field name="prefix">PDT</field>
            <field name="padding">9</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>

        <record id="person_re_enable_transaction_seq" model="ir.sequence">
            <field name="name">seta.person.re.enable.transaction sequence</field>
            <field name="code">person.re.enable.transaction</field>
            <field name="prefix">PDE</field>
            <field name="padding">9</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="False" name="company_id"/>
        </record>
    </data>
</odoo>