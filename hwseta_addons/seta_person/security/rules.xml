<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
         <record id="seta_person_see_own" model="ir.rule">
	        <field name="name">Own Person Profile</field>
	        <field ref="model_seta_person" name="model_id"/>
	        <field name="domain_force">[('user_id', '=', user.id)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_master_ext'))]"/>
			<field name="perm_write" eval="False"/>
	        <field name="perm_create" eval="False"/>
	        <field name="perm_unlink" eval="False"/>
	    </record>
		<record id="seta_person_see_all" model="ir.rule">
	        <field name="name">ALL Person Profile</field>
	        <field ref="model_seta_person" name="model_id"/>
	        <field name="domain_force">[(1, '=', 1)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_admin'))]"/>
	    </record>


		<record id="seta_person_disable_see_own_transaction" model="ir.rule">
	        <field name="name">Own Person Disable Transactions</field>
	        <field ref="model_seta_person_disable_transaction" name="model_id"/>
	        <field name="domain_force">[('user_id', '=', user.id)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_disable_transaction_ext'))]"/>
			<field name="perm_write" eval="False"/>
	        <field name="perm_create" eval="False"/>
	        <field name="perm_unlink" eval="False"/>
	    </record>

		<record id="seta_person_disable_see_all_transaction" model="ir.rule">
	        <field name="name">ALL Person Disable Transactions</field>
	        <field ref="model_seta_person_disable_transaction" name="model_id"/>
	        <field name="domain_force">[(1, '=', 1)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_admin'))]"/>
	    </record>

		<record id="seta_person_re_enable_see_own_transaction" model="ir.rule">
	        <field name="name">Own Person Re-Enable Transactions</field>
	        <field ref="model_seta_person_re_enable_transaction" name="model_id"/>
	        <field name="domain_force">[('user_id', '=', user.id)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_re_enable_transaction_ext'))]"/>
			<field name="perm_write" eval="False"/>
	        <field name="perm_create" eval="False"/>
	        <field name="perm_unlink" eval="False"/>
	    </record>

		<record id="seta_person_re_enable_see_all_transaction" model="ir.rule">
	        <field name="name">ALL Person Re-Enable Transactions</field>
	        <field ref="model_seta_person_re_enable_transaction" name="model_id"/>
	        <field name="domain_force">[(1, '=', 1)]</field>
	        <field name="groups" eval="[(4, ref('seta_person.group_person_admin'))]"/>
	    </record>


    </data>
</odoo>